{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src/plot"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "destination": "./docs/document",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./docs/templates/jaguarjs"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false
  }
}